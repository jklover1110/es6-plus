{
  "extends": "@antfu",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "antfu/top-level-function": 0,
    "eqeqeq": 0,
    "new-parens": [2, "never"],
    "no-void": 0,
    "no-extend-native": 0,
    "no-console": 0,
    "no-new-func": 0,
    "no-new-wrappers": 0,
    "no-global-assign": 0,
    "no-shadow-restricted-names": 0,
    "no-undef": 0,
    "no-with": 0,
    "@typescript-eslint/no-use-before-define": 0,
    "no-restricted-syntax": 0,
    "prefer-const": 0,
    "prefer-spread": 0,
    "prefer-rest-params": 0,
    "no-useless-call": 0,
    "unicorn/throw-new-error": 0,
    "@typescript-eslint/no-invalid-this": 0,
    "@typescript-eslint/no-this-alias": 0
  }
}
